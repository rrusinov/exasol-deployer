{
  "sut_name": "libvirt-3n",
  "description": "Libvirt 3-node cluster configuration",
  "provider": "libvirt",
  "parameters": {
    "cluster_size": 3,
    "libvirt_memory": 8,
    "libvirt_vcpus": 4,
    "data_volumes_per_node": 1,
    "data_volume_size": 100,
    "root_volume_size": 50
  },
  "requirements": {
    "libvirt_available": true,
    "kvm_available": true,
    "min_memory_gb": 32
  }
}
